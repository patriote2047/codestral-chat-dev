{
  "name": "exemple-test",
  "version": "1.0.0",
  "description": "Exemple de configuration pour les tests",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "nodemon index.js",
    "start": "node index.js",
    "build": "webpack --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
