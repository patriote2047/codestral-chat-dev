{
    "name": "codyman",
    "version": "1.0.0",
    "description": "Outils pour la gestion de code",
    "scripts": {
        "dev": "next dev",
        "dev:watch": "nodemon --watch src --watch public --watch next.config.js --exec \"next dev\"",
        "dev:managed": "node scripts/dev-server.js",
        "build": "next build",
        "start": "next start",
        "lint": "next lint --fix",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "type-check": "tsc --noEmit",
        "test": "jest --watchAll=false --coverage",
        "test:staged": "jest --bail --findRelatedTests",
        "test:check-missing": "node scripts/check-missing-tests.mjs",
        "test:move": "node scripts/move-tests.mjs",
        "test:full": "npm run test && npm run test:check-missing",
        "list:dirs": "node scripts/list-directories.mjs",
        "list:files": "node scripts/list-files.mjs",
        "list:functions": "node scripts/list-functions.mjs",
        "list:lines": "node scripts/count-lines.mjs",
        "list:root": "node scripts/list-root.mjs",
        "list:config": "node scripts/list-config.mjs",
        "list:deps": "node scripts/list-deps.mjs",
        "list:commands": "node scripts/list-commands.mjs",
        "list:commands:simple": "node scripts/list-commands-simple.mjs",
        "list:commands:default": "node scripts/default-command-desc.mjs",
        "list:deps:suggestions": "node scripts/deps-suggestions.mjs",
        "analyze:unused-deps": "node scripts/analyze-unused-deps.mjs",
        "add:command:desc": "node scripts/add-command-desc.mjs",
        "check:deps": "node scripts/check-deps.mjs",
        "show:time": "node scripts/show-time.mjs",
        "chrono": "node scripts/chronometre.mjs",
        "validate": "npm-run-all --parallel type-check lint format",
        "clean": "rimraf .next out",
        "clean:all": "npm-run-all clean && rimraf node_modules",
        "reinstall": "npm-run-all clean:all && npm install",
        "deps:check": "npm-check-updates",
        "deps:update": "npm-check-updates -u && npm install",
        "deps:audit": "npm audit fix",
        "update": "npm-run-all deps:check deps:update deps:audit",
        "prepare": "husky"
    },
    "dependencies": {
        "date-fns": "^4.1.0",
        "next": "15.1.6",
        "node-gzip": "^1.1.2",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "styled-components": "6.1.14",
        "typescript": "5.3.3",
        "uuid": "^11.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.26.7",
        "@babel/parser": "^7.26.7",
        "@babel/preset-env": "^7.26.7",
        "@babel/preset-react": "^7.26.3",
        "@babel/preset-typescript": "^7.26.0",
        "@babel/types": "^7.24.0",
        "@commitlint/cli": "^19.7.1",
        "@commitlint/config-conventional": "^19.7.1",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@types/jest": "29.5.14",
        "@types/node": "22.13.1",
        "@types/prettier": "^3.0.0",
        "@types/react": "18.3.18",
        "@types/react-dom": "18.3.5",
        "@types/socket.io": "^3.0.2",
        "@types/socket.io-client": "^3.0.0",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "babel-jest": "29.7.0",
        "bufferutil": "^4.0.9",
        "chai": "^5.1.2",
        "chalk": "^5.4.1",
        "eslint": "^8.56.0",
        "eslint-config-next": "^14.2.23",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^5.1.0",
        "glob": "^11.0.1",
        "husky": "^9.1.7",
        "identity-obj-proxy": "3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "mocha": "^11.1.0",
        "nodemon": "^3.1.9",
        "npm-check-updates": "^16.14.20",
        "npm-run-all": "4.1.5",
        "prettier": "^3.4.2",
        "rimraf": "5.0.10",
        "utf-8-validate": "^6.0.5"
    }
}
